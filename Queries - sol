1-select * from emp;
2-select * from dept;
3-select ename , job from emp;
4-select ename , sal from emp;
5-select empno , ename , sal , comm , sal+nvl(comm,0) as "total salary" from emp;
6-select ename , 12 *(sal+nvl(comm,0)) as "annual sal" from emp;
7-select ename from emp where deptno = 10;
8-select ename from emp where job = 'CLERK' and sal > 3000;
9-select empno , ename from emp where comm is not null;
10-select empno , ename from emp where comm is null;
11-select ename from emp where job = 'CLERK' or job = 'SALESMAN' or job = 'ANALYST' and sal > 3000;
12-select ename from emp where to_char(sysdate,'YYYY') - to_char(hiredate,'YYYY')>=5;
13-select ename from emp where hiredate < '30-JUN-1990' or hiredate > '31-DEC-90';
14-select sysdate from dual;
15-select username from all_users;
16-select tname from tab;
17-show user
18-select ename from emp where deptno in (10,20,40) or job in ('CLERKS','SALESMAN','ANALYST')
19-select ename from emp where ename like 'S%';
20-select ename from emp where ename like '%S';
21-select ename from emp where ename like '_A%';
22-select ename from emp where length(ename)=5;
23-select ename from emp where job not in('MANAGER');
24-select ename from emp where job not in ('SALESMAN','CLERK','ANALYST');
25-set pause on
26-select count(*) from emp;
27-select sum(sal) from emp;
28-select max(sal) from emp;
29-select min(sal) from emp;
30-select avg(sal) from emp;
31-select max(sal) from emp;
32-select max(sal) from emp where deptno = 20;
33-select min(sal) from emp where job = 'SALESMAN';
34-select avg(sal) from emp where job = 'MANAGER';
35-select sum(sal) from emp where job='ANALYST' and deptno = 40;
36-select ename from emp order by sal;
37-selec ename from emp ordery by sal desc;
38-select ename from emp order by ename;
39-select empno , ename , deptno , sal from emp order by ename ,deptno , sal;
40-select ename , sal*12 from emp order by sal desc;
41-select ename , sal , sal/100*15 as hra, sal/100*5 as pf,sal/100*10 as da , sal+sal/100*15+sal/100*10-sal/100*5 as total from emp;  
42-select deptno,count(deptno) from emp group by deptno;
43-select job , count(job) from emp group by job;
44-select deptno , sum(sal) from emp group by deptno;
45-select deptno , max(sal) from emp group by deptno;
